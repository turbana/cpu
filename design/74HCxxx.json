{
	"7400": {
		"desc": "Quad 2-bit NAND gate",
		"delay": 7,
		"package": "DIP14",
		"names": ["1A", "1B", "1Y", "2A", "2B", "2Y", "GND", "3Y", "3A", "3B", "4Y", "4A", "4B", "VCC"],
		"assign": {"?Y": "~(?A & ?B)"}
	},
	"7408": {
