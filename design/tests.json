{
	"execute_barrel_shifter": {
		"delay": 162,
		"inputs": {
			"_E_A":    {"width": 16, "alias": "n"},
			"_E_B":    {"width": 16},
			"D_ALUop": {"width": 3}
		},
		"assert": "(D_ALUop & 3) != 3",
		"internal": {
			"s": "_E_B & 0xF",
			"r": "(~D_ALUop & 2) >> 1",
			"a": "D_ALUop & 1"
		},
		"outputs": {
			"_E_S": {
				"width": 16,
				"formula": "((n>>s)|((2**16 - 2**(16-s)) * (n>>15))) if r and a else ((n>>s) if r else (n<<s))"
			}
		}
	},
	"execute_alu": {
		"delay": 1000,
		"inputs": {
			"_E_A": {"width": 16, "alias": "a"},
			"_E_B": {"width": 16, "alias": "b"},
			"D_ALUop": {"width": 3, "alias": "op"}
		},
		"assert": "D_ALUop != 7",
		"internal": {
			"s": "_E_B & 0xF",
			"r": "(~D_ALUop & 2) >> 1",
			"ar": "D_ALUop & 1"
		},
		"outputs": {
			"_E_D": {
				"width": 16,
				"formula": "a+b if op==0 else (a&b if op ==1 else (a|b if op==2 else (a^b if op==3 else (((a>>s)|((2**16 - 2**(16-s)) * (a>>15))) if r and ar else ((a>>s) if r else (a<<s))))))"
			}
		}
	},
	"ic7408": {
		"delay": 27,
		"inputs": {
			"A": {"width": 4},
			"B": {"width": 4}
		},
		"outputs": {
			"X": {"width": 4, "formula": "A & B"}
		}
	},
	"ic7432": {
		"delay": 10,
		"inputs": {
			"A": {"width": 4},
			"B": {"width": 4}
		},
		"outputs": {
			"X": {"width": 4, "formula": "A | B"}
		}
	},
	"ic7486": {
		"delay": 30,
		"inputs": {
			"A": {"width": 4},
			"B": {"width": 4}
		},
		"outputs": {
			"X": {"width": 4, "formula": "A ^ B"}
		}
	},
	"ic74153": {
		"delay": 34,
		"inputs": {
			"A": {"width": 2},
			"B": {"width": 2},
			"C": {"width": 2},
			"D": {"width": 2},
			"S": {"width": 2}
		},
		"outputs": {
			"X": {"width": 2, "formula": "A if S==0 else (B if S==1 else (C if S==2 else D))"}
		}
	},
	"ic74283": {
		"delay": 320,
		"inputs": {
			"A":   {"width": 16},
			"B":   {"width": 16},
			"Cin": {"width": 1}
		},
		"outputs": {
			"X":    {"width": 16, "formula": "A + B + Cin"},
			"Cout": {"width": 1,  "formula": "(A + B + Cin) >> 16"}
		}
	}
}
